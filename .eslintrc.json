{
    "env": {
      "browser": true,
      "es2021": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:svelte/recommended",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "plugins": ["svelte"],
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "warn"
    },
    "overrides": [
      {
        "files": ["*.svelte"],
        "parser": "svelte-eslint-parser"
      }
    ]
  }