{
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "printWidth": 100,
  "tabWidth": 2,
  "arrowParens": "always",
  "bracketSpacing": true,
  "plugins": ["prettier-plugin-svelte", "@prettier/plugin-xml"],
  "svelteSortOrder": "scripts-options-markup-styles",
  "svelteStrictMode": false,
  "svelteIndentScriptAndStyle": true
}
